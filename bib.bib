@article{stoll2009,
	title = {Transcriptional profiling of the endosymbiont {\textit{Blochmannia floridanus}} during different developmental stages of its holometabolous ant host},
	volume = {11},
	copyright = {Journal compilation © 2008 Society for Applied Microbiology and Blackwell Publishing Ltd. No claim to original German government works},
