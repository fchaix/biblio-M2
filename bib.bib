	author = {Liu, Qiu-Ning and Zhu, Bao-Jian and Dai, Li-Shang and Fu, Wei-Wei and Lin, Kun-Zhang and Liu, Chao-Liang},
	month = aug,
	year = {2013},
	keywords = {Antheraea pernyi, Expression, Heat shock protein, Temperature},
	pages = {848--854},
	file = {Liu et al. - 2013 - Overexpression of small heat shock protein 21 prot.pdf:/home/ego/.mozilla/firefox/e94dk8of.default/zotero/storage/9HGJRK3E/Liu et al. - 2013 - Overexpression of small heat shock protein 21 prot.pdf:application/pdf;ScienceDirect Snapshot:/home/ego/.mozilla/firefox/e94dk8of.default/zotero/storage/586PESUC/S0022191013001339.html:text/html}
}
