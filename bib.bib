	author = {Andersen, Janne Pleidrup and Schwartz, Alex and Gramsbergen, Jan Bert and Loeschcke, Volker},
	month = nov,
	year = {2006},
	keywords = {Ecdysone, {HPLC}, Juvenile hormone, Stress response, Thermal limits},
	pages = {1163--1170},
